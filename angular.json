{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"angular-svg-icon-preloader-demo": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "apps/angular-svg-icon-preloader-demo",
			"sourceRoot": "apps/angular-svg-icon-preloader-demo/src",
			"prefix": "angular-svg-icon-preloader",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:application",
					"options": {
						"outputPath": {
							"base": "dist/apps/angular-svg-icon-preloader-demo"
						},
						"index": "apps/angular-svg-icon-preloader-demo/src/index.html",
						"polyfills": ["zone.js"],
						"tsConfig": "apps/angular-svg-icon-preloader-demo/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/angular-svg-icon-preloader-demo/src/favicon.ico",
							"apps/angular-svg-icon-preloader-demo/src/assets"
						],
						"styles": [
							"apps/angular-svg-icon-preloader-demo/src/styles.scss"
						],
						"scripts": [],
						"browser": "apps/angular-svg-icon-preloader-demo/src/main.ts"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							],
							"fileReplacements": [
								{
									"replace": "apps/angular-svg-icon-preloader-demo/src/environments/environment.ts",
									"with": "apps/angular-svg-icon-preloader-demo/src/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "angular-svg-icon-preloader-demo:build:production"
						},
						"development": {
							"buildTarget": "angular-svg-icon-preloader-demo:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"buildTarget": "angular-svg-icon-preloader-demo:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "apps/angular-svg-icon-preloader-demo/src/test.ts",
						"polyfills": ["zone.js", "zone.js/testing"],
						"tsConfig": "apps/angular-svg-icon-preloader-demo/tsconfig.spec.json",
						"karmaConfig": "apps/angular-svg-icon-preloader-demo/karma.conf.js",
						"inlineStyleLanguage": "scss",
						"assets": [
							"apps/angular-svg-icon-preloader-demo/src/favicon.ico",
							"apps/angular-svg-icon-preloader-demo/src/assets"
						],
						"styles": [
							"apps/angular-svg-icon-preloader-demo/src/styles.scss"
						],
						"scripts": []
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"apps/angular-svg-icon-preloader-demo/**/*.ts",
							"apps/angular-svg-icon-preloader-demo/**/*.html"
						]
					}
				}
			}
		},
		"angular-svg-icon-preloader": {
			"projectType": "library",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "libs/angular-svg-icon-preloader",
			"sourceRoot": "libs/angular-svg-icon-preloader/src",
			"prefix": "angular-svg-icon-preloader",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"tsConfig": "libs/angular-svg-icon-preloader/tsconfig.lib.json",
						"project": "libs/angular-svg-icon-preloader/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "libs/angular-svg-icon-preloader/tsconfig.lib.prod.json"
						}
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"main": "libs/angular-svg-icon-preloader/src/test-setup.ts",
						"polyfills": ["zone.js", "zone.js/testing"],
						"tsConfig": "libs/angular-svg-icon-preloader/tsconfig.spec.json",
						"karmaConfig": "libs/angular-svg-icon-preloader/karma.conf.js",
						"inlineStyleLanguage": "scss"
					}
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"libs/angular-svg-icon-preloader/**/*.ts"
						]
					}
				}
			}
		},
		"angular-svg-icon-preloader-demo-e2e": {
			"projectType": "application",
			"root": "apps/angular-svg-icon-preloader-demo-e2e",
			"sourceRoot": "apps/angular-svg-icon-preloader-demo-e2e/src",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "apps/angular-svg-icon-preloader-demo-e2e/protractor.conf.js"
					},
					"configurations": {
						"production": {
							"devServerTarget": "angular-svg-icon-preloader-demo:serve:production"
						},
						"development": {
							"devServerTarget": "angular-svg-icon-preloader-demo:serve:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": [
							"apps/angular-svg-icon-preloader-demo-e2e/**/*.ts"
						]
					}
				}
			}
		}
	},
	"cli": {
		"analytics": "947991e5-718d-4b38-95cb-d4d2563cb533",
		"packageManager": "npm"
	},
	"schematics": {
		"@schematics/angular:component": {
			"type": "component"
		},
		"@schematics/angular:directive": {
			"type": "directive"
		},
		"@schematics/angular:service": {
			"type": "service"
		},
		"@schematics/angular:guard": {
			"typeSeparator": "."
		},
		"@schematics/angular:interceptor": {
			"typeSeparator": "."
		},
		"@schematics/angular:module": {
			"typeSeparator": "."
		},
		"@schematics/angular:pipe": {
			"typeSeparator": "."
		},
		"@schematics/angular:resolver": {
			"typeSeparator": "."
		}
	}
}
